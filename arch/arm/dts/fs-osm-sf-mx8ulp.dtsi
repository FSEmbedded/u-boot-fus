	/* 0-4 */ "SD_eMMC_RST", "SD_eMMC_CMD", "SD_eMMC_CLK", "SD_eMMC_D7", "SD_eMMC_D6",
	/* 5-9 */ "SD_eMMC_D5", "SD_eMMC_D4", "SD_eMMC_D3", "SD_eMMC_D2", "SD_eMMC_D1",
	/* 10-14 */ "SD_eMMC_D0", "SD_eMMC_DS", "USB_A_ID", "USB_A_EN", "USB_A_OC",
	/* 15-19 */ "SDIO_A_PWR_EN", "SDIO_A_CD", "SDIO_A_WP", "SDIO_A_D3", "SDIO_A_D2",
	/* 20-24 */ "SDIO_A_D1", "SDIO_A_D0", "SDIO_A_CLK", "SDIO_A_CMD", "",
	/* 25-29 */ "", "", "", "", "",
	/* 30-31 */ "","";
	status = "okay";
};

&gpioe {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpioe>;
	gpio-line-names = 
	/* 0-4 */ "GPIO_C_0", "GPIO_C_1", "USB_B_EN", "ETH_A_RMII_TX_CLK", "USB_B_ID",
	/* 5-9 */ "USB_B_OC", "UART_CON_TX", "UART_CON_RX", "UART_A_CTS", "UART_A_RTS",
	/* 10-14 */ "UART_A_TX", "UART_A_RX", "I2C_A_SCL", "I2C_A_SDA", "ETH_MDIO",
	/* 15-19 */ "ETH_MDC", "ETH_A_RMII_TX_EN(_ER)", "ETH_A_RMII_RX_ER", "ETH_A_RMII_RX_DV(_ER)", "ETH_A_RMII_RX_CLK",
	/* 20-24 */ "ETH_A_RMII_RXD1", "ETH_A_RMII_RXD0", "ETH_A_RMII_TXD1", "ETH_A_RMII_TXD0", "",
